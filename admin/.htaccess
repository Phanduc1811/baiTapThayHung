AddDefaultCharset UTF-8
RewriteEngine on
RewriteRule ^logout.html$ index.php?controller=Login&action=logout [L]
RewriteRule ^login.html$ index.php?controller=Login&action=index [L]
RewriteRule ^login_dologin.php$ index.php?controller=Login&action=dologin

# Rewrite mode tin tuc #
RewriteRule ^index.html index.php
RewriteRule ^404.html 404.php
RewriteRule ^basic_table.html basic_table.php
RewriteRule ^form_component.html form_component.php
RewriteRule ^form_validation.html form_validation.php
RewriteRule ^login.html login.php
RewriteRule ^responsive_table.html responsive_table.php